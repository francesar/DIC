class BinopTest{
	func int main(){
		// Add 3+2 should be 5.
		printint(3+2);

		// Subtract 3-2 should be 1.
		printint(3-2);

		// Multiply 3*2 should be 6.
		printint(3*2);

		// Divide 3/2 should be 1.
		printint(3/2);

		if(true && true){
			printstr("&& works.");
		}

		if (true || false){
			printstr("|| works.");
		}

        if(false || false) {
            printstr("should not print");
        }

        if(true && false) {
            printstr("should not print");
        }

		if (2 == 2){
			printstr("== works.");
		}

		if (3 != 2){
			printstr("!= works.");
		}

		if (2 < 3){
			printstr("< works.");
		}

		if (2 <= 3){
			printstr("<= works.");
		}

		if (3 > 2){
			printstr("> works.");
		}

		if (3 < 2){
			printstr("This shouldn't print.");
		}

		if (3 >= 2){
			printstr(">= works.");
		}

		if (3 > 2 && 3 != 2){
			printstr(">, &&, and != work together.");
		}
		printstr("success");
		return 0;
	}
}